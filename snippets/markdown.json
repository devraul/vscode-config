{
	"markdown JSCode": {
		"prefix": "jscode",
		"body": [
			"```javascript",
			"${1:your code here}",
			"```"
		],
		"description": "code block for javascript"
	},
	"codeblock": {
		"prefix": "code",
		"body": [
			"```${1:language}",
			"$2",
			"```"
		],
		"description": "code block"
	},
	"comment code snippet": {
		"prefix": "cmt",
		"body": [
			"/* $1 */"
		],
		"description": "big comment"
	},
	"Check box": {
		"prefix": "ck",
		"body": [
			"- [ ] $1"
		],
		"description": "Create a check box"
	},
	"Link": {
		"prefix": "link",
		"body": [
			"[$1]($2)"
		],
		"description": "Create a check box"
	}
}